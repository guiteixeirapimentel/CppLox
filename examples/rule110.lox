{
    // 7
    if(x1 == "x" and x2 == "x" and x3 == "x")
    {
        return " ";
    }

    // 6
    if(x1 == "x" and x2 == "x" and x3 == " ")
    {
        return "x";
    }

    // 5
    if(x1 == "x" and x2 == " " and x3 == "x")
    {
        return "x";
    }

    // 4
    if(x1 == "x" and x2 == " " and x3 == " ")
    {
        return " ";
    }

    // 3
    if(x1 == " " and x2 == "x" and x3 == "x")
    {
        return "x";
    }

    // 2
    if(x1 == " " and x2 == "x" and x3 == " ")
    {
        return "x";
    }

    // 1
    if(x1 == " " and x2 == " " and x3 == "x")
    {
        return "x";
