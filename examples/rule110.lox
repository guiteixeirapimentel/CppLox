    // 0
    if(x1 == " " and x2 == " " and x3 == " ")
    {
        return " ";
    }
}

fun advanceGeneration()
{
    newBoard = " ";
    for(var i = 1; i < BOARD_SIZE - 1; i = i + 1)
    {
        newBoard = newBoard + getVal(curBoard[i - 1], curBoard[i], curBoard[i + 1]);
    }

    newBoard = newBoard + "x";

    curBoard = newBoard;
}

fun printCurrentGen()
{
    print(curBoard);
}

var t1 = clock();

for(var i = 0; i < MAX_GENS; i = i + 1)
{
    advanceGeneration();
    printCurrentGen();
}

var t2 = clock();

print("110 rule took the following seconds");
print(t2 - t1);