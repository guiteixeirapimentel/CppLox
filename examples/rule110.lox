var BOARD_SIZE = 100;

var curBoard = "";

for(var i = 0 ; i < BOARD_SIZE - 1; i = i + 1)
{
    curBoard = curBoard + " ";
}
curBoard = curBoard + "x";

var newBoard = curBoard;

var MAX_GENS = 100;

