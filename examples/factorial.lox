fun fac(n) { if(n > 1) { return (fac(n-1)*n); } return 1; }

print(fac(10));

if(fac(3) == 6)
{
    print("success 3!");

}
else
{
    print("failed expected 6 got: ");
    print(fac(10));
}

if(fac(10) == 3628800)
{
    print("success 10!");
}
else
{
    print("failed expected 3628800 got ");
    print(fac(10));
}
